{
    "author": "Noah May",
    "license": "MIT",
    "name": "react-express-apollo-quickstart",
    "version": "1.0.0",
    "description": "Boilerplate to get up and running with React, Express, and GraphQL from Apollo",
    "main": "dist/server.bundle.js",
    "scripts": {
        "start": "node dist/server.bundle.js",
        "build": "npm-run-all -s generate build:once",
        "build:once": "webpack",
        "build:watch": "webpack -w",
        "generate": "graphql-codegen --config ./src/graphql/codegen.yml",
        "dev": "npm-run-all -p generate build:watch server:run",
        "client": "npm-run-all -p client:*",
        "client:build": "webpack -w --config-name client",
        "client:run": "webpack-dev-server --inline --config-name client",
        "server": "npm-run-all -p server:*",
        "server:build": "webpack -w --config-name server",
        "server:run": "nodemon --ignore src -w dist/server.bundle.js",
        "test": "echo Whoops! No testing setup yet..."
    },
    "dependencies": {
        "@apollo/react-hooks": "^3.1.5",
        "@graphql-codegen/cli": "^1.13.3",
        "@types/helmet": "0.0.46",
        "@types/isomorphic-fetch": "0.0.35",
        "@types/react": "^16.9.34",
        "@types/react-dom": "^16.9.7",
        "@types/react-router-dom": "^5.1.5",
        "apollo-boost": "^0.4.7",
        "apollo-cache-inmemory": "^1.6.5",
        "apollo-client": "^2.6.8",
        "apollo-link-error": "^1.1.13",
        "apollo-server-express": "^2.14.2",
        "bootstrap": "^4.4.1",
        "compression": "^1.7.4",
        "express": "^4.17.1",
        "graphql": "^14.6.0",
        "graphql-import": "^1.0.2",
        "graphql-tag": "^2.10.3",
        "graphql-tools": "^5.0.0",
        "helmet": "^3.22.0",
        "isomorphic-fetch": "^2.2.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.1.2"
    },
    "devDependencies": {
        "@graphql-codegen/typescript": "^1.13.3",
        "@graphql-codegen/typescript-resolvers": "^1.13.3",
        "@types/compression": "^1.7.0",
        "@types/express": "^4.17.6",
        "@types/node": "^13.13.4",
        "@types/webpack": "^4.41.12",
        "@types/webpack-dev-server": "^3.10.1",
        "css-loader": "^3.5.3",
        "dotenv": "^8.2.0",
        "node-sass": "^4.14.0",
        "nodemon": "^2.0.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.0.5",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.2.0",
        "ts-loader": "^7.0.1",
        "ts-node": "^8.9.1",
        "typescript": "^3.8.3",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3",
        "webpack-node-externals": "^1.7.2"
    }
}
