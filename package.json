{
    "author": "Noah May",
    "license": "MIT",
    "name": "react-express-apollo-quickstart",
    "version": "1.0.0",
    "description": "Boilerplate to get up and running with React, Express, and GraphQL from Apollo",
    "main": "dist/server.bundle.js",
    "scripts": {
        "start": "node dist/server.bundle.js",
        "build": "npm-run-all -s generate build:once",
        "build:once": "webpack",
        "build:watch": "webpack -w",
        "generate": "graphql-codegen --config ./src/graphql/codegen.yml",
        "dev": "npm-run-all -p generate build:watch server:run",
        "client": "npm-run-all -p client:*",
        "client:build": "webpack -w --config-name client",
        "client:run": "webpack-dev-server --inline --config-name client",
        "server": "npm-run-all -p server:*",
        "server:build": "webpack -w --config-name server",
        "server:run": "nodemon --ignore src -w dist/server.bundle.js",
        "test": "echo Whoops! No testing setup yet..."
    },
    "dependencies": {
        "@apollo/react-hooks": "^3.1.3",
        "@graphql-codegen/cli": "^1.10.0",
        "@types/helmet": "0.0.45",
        "@types/isomorphic-fetch": "0.0.35",
        "@types/react": "^16.9.17",
        "@types/react-dom": "^16.9.4",
        "@types/react-router-dom": "^5.1.3",
        "apollo-boost": "^0.4.7",
        "apollo-cache-inmemory": "^1.6.5",
        "apollo-client": "^2.6.8",
        "apollo-link-error": "^1.1.12",
        "apollo-server-express": "^2.9.15",
        "bootstrap": "^4.4.1",
        "compression": "^1.7.4",
        "express": "^4.17.1",
        "graphql": "^14.5.8",
        "graphql-import": "^0.7.1",
        "graphql-tag": "^2.10.1",
        "graphql-tools": "^4.0.6",
        "helmet": "^3.21.2",
        "isomorphic-fetch": "^2.2.1",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-router-dom": "^5.1.2"
    },
    "devDependencies": {
        "@graphql-codegen/typescript": "^1.10.0",
        "@graphql-codegen/typescript-resolvers": "^1.10.0",
        "@types/express": "^4.17.2",
        "@types/node": "^13.1.2",
        "@types/webpack": "^4.41.1",
        "@types/webpack-dev-server": "^3.9.0",
        "css-loader": "^3.4.0",
        "dotenv": "^8.2.0",
        "node-sass": "^4.13.0",
        "nodemon": "^2.0.2",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.19.1",
        "sass-loader": "^8.0.0",
        "style-loader": "^1.1.2",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.5.4",
        "typescript": "^3.7.4",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.10.1",
        "webpack-node-externals": "^1.7.2"
    }
}
